{
	"name": "firegamesbot",
	"version": "1.0.0",
	"description": "a telegram bot for searching video games in firegames",
	"main": "index.js",
	"scripts": {
		"build": "babel . --ignore node_modules,dist -d dist --presets es2015,stage-0",
		"start": "NODE_ENV=production nodemon index.js --exec babel-node --presets es2015,stage-0",
		"serve": "node dist/index.js",
		"heroku-postbuild": "babel app/*.js app/features/*.js -d dist --presets es2015,stage-0",
		"dev": "npm run build && NODE_ENV=development nodemon --exec babel-node app/index.js",
		"test": "echo \"Error: no test specified\" && exit 1"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/marc-se/firegamesbot.git"
	},
	"keywords": [
		"firegames",
		"firebase",
		"telegram",
		"bot"
	],
	"author": "Marcel Sebald",
	"license": "ISC",
	"bugs": {
		"url": "https://github.com/marc-se/firegamesbot/issues"
	},
	"homepage": "https://github.com/marc-se/firegamesbot#readme",
	"dependencies": {
		"dotenv": "^7.0.0",
		"express": "^4.16.4",
		"firebase": "^5.9.1",
		"nodemon": "^1.18.10",
		"telegraf": "^3.27.1"
	},
	"devDependencies": {
		"babel-cli": "^6.26.0",
		"babel-polyfill": "^6.26.0",
		"babel-preset-es2015": "^6.24.1",
		"babel-preset-stage-0": "^6.24.1",
		"eslint": "5.15.3",
		"eslint-plugin-import": "2.16.0",
		"rimraf": "^2.6.3"
	}
}
