  "scripts": {
    "build": "tsc",
    "start": "npm run build && NODE_ENV=production node dist/index.js",
    "serve": "node dist/index.js",
    "clean": "rimraf dist",
    "dev": "NODE_ENV=development nodemon --exec ts-node app/index.ts",
